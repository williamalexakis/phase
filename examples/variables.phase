entry {

    -- Grouped variable initializations
    int (a, b, c) = (5, 10, 15)
    str (x, y, z) = ("hello", "world", "!")

    -- Declarations
    str i
    str j
    str k

    -- Cross assignments
    i = x
    j = y
    k = z

    -- Print results
    out("a, b, c:")
    out(a)
    out(b)
    out(c)
    out("")
    out("x, y, z:")
    out(i)
    out(j)
    out(k)

}
