entry {
    -- Grouped initializations
    let (a, b, c): int = (5, 10, 15)
    let (x, y, z): str = ("hello", "world", "!")

    -- Grouped declarations
    let (p, q, r): int
    let (i, j, k): str

    -- Cross assignments
    p = a
    q = b
    r = c
    i = x
    j = y
    k = z

    -- Print results
    out("a, b, c:")
    out(p)
    out(q)
    out(r)
    out("")
    out("x, y, z:")
    out(i)
    out(j)
    out(k)
}
